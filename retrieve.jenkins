#!groovy
//Disableotherbuilds
properties([disableConcurrentBuilds()])


pipeline{
agent{
label'master'
}

options{
buildDiscarder(logRotator(numToKeepStr:'5',artifactNumToKeepStr:'20'))
timestamps()
}

stages{

stage("One"){
steps{
sh'sshroot@jenkins-agent1\'hostname\''
}
}
stage("Two"){
steps{
sh'sshroot@jenkins-agent1\'uptime\''
}
}
}
}
